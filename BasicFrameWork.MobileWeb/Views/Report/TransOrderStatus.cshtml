@model BasicFramework.MobileWeb.Models.Report.TransOrderModel
@{
    ViewBag.PageTitle = "运单状态统计";
    Layout = "~/Views/_MyLayoutPage.cshtml";
}
@Html.HiddenFor(m => m.transOrderRequest.StartTime)
@Html.HiddenFor(m => m.transOrderRequest.EndTime)
<div class="container">
    <div class="row">
        <div class="col-xs-6 col-sm-6 col-md-6">
            <input readonly="true" id="start_ActualDeliveryDate" class="datepicker form-control" data-date-format="yyyy/mm/dd" placeholder="starttime" data-date-autoclose="true" data-date-disabletouchkeyboard="true" data-date-todayhighlight="true"  data-date-showonfocus="false">
        </div>
        <div class="col-xs-6 col-sm-6 col-md-6">
            <input readonly="true" id="end_ActualDeliveryDate" class="datepicker form-control" data-date-format="yyyy/mm/dd" placeholder="endtime" data-date-autoclose="true" data-date-disabletouchkeyboard="true" data-date-todayhighlight="true" data-date-showonfocus="false">
        </div>
    </div>
</div>
<br />

<div class="container">
    <input id="txt_search" type="text" class="txt_search" placeholder="输入关键字">
    <button type="button" class="btn_search" onclick="getsearch()" id="searchButton"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
</div>

<div id="container" style="min-width: 300px; height: 400px; margin: 10px;"></div>
<div id="containers" style="min-width: 300px; height: 400px; margin: 10px; display: none"></div>
<div id="containers3" style="min-width: 300px; height: 400px; margin: 10px; display: none">
</div>
@section myCss{
    <link href="http://cdn.bootcss.com/bootstrap-datepicker/1.4.0/css/bootstrap-datepicker.min.css" rel="stylesheet">
}

@section myJs{
    <script src="http://cdn.bootcss.com/bootstrap-datepicker/1.4.0/js/bootstrap-datepicker.min.js"></script>
    @*  <script src="http://code.highcharts.com/highcharts.js"></script> 
    <script src="http://cdn.hcharts.cn/highcharts/modules/exporting.js"></script>*@
    <script src="~/js/Common/Highcharts/highcharts.js"></script>
    <script src="~/js/Common/Highcharts/exporting.js"></script>
    <script src="~/js/Common/Highcharts/gray.js"></script>

    <script src="~/js/Common/Highcharts/drilldown.js"></script>
    <script src="~/js/Report/TransOrderStatus.js"></script>
    <script src="~/js/Common/Framework/json2.js"></script>
    <script src="~/js/Common/Framework/jsrender.js"></script>

}
@*@Url.LoadHighchartsJS()
@Url.Json2()*@
<script id="Evaluation" type="text/x-jsrender">
    <ul class="list-group">
        <li class="list-group-item">
            <a>
                <h4><strong>运单号 ： {{:CustomerOrderNumber}}</strong></h4>
            </a>
            <div class="td_lt"><strong> 客户：</strong><span class="text-danger">{{:CustomerName}}</span></div>
            <div class="td_lt"><strong>承运商：</strong>{{:ShipperName}}</div>
            <div class="td_lt"><strong>订单运输状态：</strong>{{:Str2}}</div>
            <div class="td_lt"><strong>发货时间：</strong>{{:ActualDeliveryDate}}</div>
            <div class="td_lt"><strong>箱数：</strong>{{:BoxNumber}}</div>
        </li>
    </ul>


</script>
