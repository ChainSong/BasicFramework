@{
    if (ViewBag.JQueryBaseJs == null)
    {
        ViewBag.JQueryBaseJs = true;
    }
    if (ViewBag.CommonJs == null)
    {
        ViewBag.CommonJs = true;
    }
    if (ViewBag.PageCssFile == null)
    {
        ViewBag.PageCssFile = true;
    }
    if (ViewBag.PageJsFile == null)
    {
        ViewBag.PageJsFile = true;
    }
}

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" /><!--禁止浏览器缩放-->

    <!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
    <title>@ViewBag.PageTitle</title>

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    

    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">
    <!-- 新 Bootstrap 核心 CSS 文件 -->
    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap.min.css">
    
    <link href="~/css/common.css" rel="stylesheet" />
    @RenderSection("myCss", false)
    <!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->
<script src="http://cdn.bootcss.com/jquery/1.11.2/jquery.min.js"></script>
  
</head>
<body>
    
    <!--顶部导航-->
    <div class="myHeader">
        <nav class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <span class="navbar-brand" onclick="javascript: window.history.go(-1);"><span class="glyphicon glyphicon-chevron-left"></span>@ViewBag.PageTitle</span>
                </div>
                @Html.Action("Menu", "Index")
            </div>
            <!--/.nav-collapse -->
        </nav>
    </div>
    <div class="myBody">
    @RenderBody()
    </div>
    <div class="footer" id="footerDiv" style="height:0px;">
        @*©2015 Runbow-虹迪股份 上ICP证030173号*@ 
    </div>
    <div class="popMsg">
        呵呵额赫尔
    </div>
    <div class="mask">
        <img src="~/img/loading.gif" />
    </div>
</body>
</html>
